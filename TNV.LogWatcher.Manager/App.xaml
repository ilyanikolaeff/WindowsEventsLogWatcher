             xmlns:local="clr-namespace:TNV.LogWatcher.Manager"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources\Converters.xaml"/>
